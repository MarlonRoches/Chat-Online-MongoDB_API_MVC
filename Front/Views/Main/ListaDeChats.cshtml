@model Back.Models.Usuario

@{
    ViewBag.Title = "ListaDeChats";
}

<h2>ListaDeChats</h2>
<ul>
    @foreach (var item in Model.Contactos)
    {
    <li>
         @item 
        <ul>
            <li style="color:springgreen">
        @Html.ActionLink("Ver Chat", "VerChat", new { Emisor = Model.User, Receptor = item})

            </li>
            <li style="color: cadetblue">

        @Html.ActionLink("Eliminar Chat", "EliminarChat", new { Emisor = Model.User, Receptor = item })

                </li>
        </ul>
    </li>
    }
</ul>
@Html.ActionLink("Modificar Datos", "Modificar");

